{
  "cli": {
    "version": ">= 10.2.1"
  },
  "build": {
    "development": {
      "extends": "base",
      "env": {
        "EXPO_PUBLIC_API_BASEURL": "https://crm.trentiums.com/api/v1/"
      },
      "channel": "development",
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "android": {
        "image": "latest"
      },
      "ios": {
        "image": "latest"
      }
    },
    "preview": {
      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6590634281",
        "appleTeamId": "Q98KG3C5AT"
      }
    }
  }
}
